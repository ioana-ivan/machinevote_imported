     </div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col\">\n        <!-- Directive v-on with clearAllVoteMachineOptions -->\n        <button\n          type=\"button\"\n          class=\"clear-button btn-lg btn-danger mx-auto d-block\"\n          v-on:click=\"clearAllVoteMachineOptions\"\n        >Effacer</button>\n      </div>\n    </div>\n\n    <!-- VoteMachine option -->\n    <div class=\"row justify-content-center\"></div>\n\n    <!-- Button Action -->\n    <!-- Directive v-bind with isCreateVoteMachineDisabled() -->\n    <div class=\"row\">\n      <div class=\"col\">\n        <!-- Directive v-on with createVoteMachine -->\n        <button\n          type=\"button\"\n          class=\"validate-button btn-lg btn-primary mx-auto d-block\"\n          v-on:click=\"createVoteMachine\"\n          :disabled=\"isCreateVoteMachineDisabled()\"\n        >Valider</button>\n      </div>\n    </div>\n\n    <div class=\"alert alert-primary\" role=\"alert\">\n      <h4 class=\"alert-heading\">Trace de Test</h4>\n      <hr>\n      <p>\n        identifiant\n        <strong>\n          <!-- Mustache with question -->\n          {{ question }}\n        </strong>\n      </p>\n      <p>TODO</p>\n    </div>\n\n    <!-- Directive v-text with errorMessage -->\n    <div class=\"error-message alert alert-danger\" role=\"alert\" v-text=\"errorMessage\"></div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"CreateVoteMachine\",\n  data() {\n    return {\n      question: \"\",\n      newVoteMachineOptionText: \"\",\n      VoteMachineOptions: [],\n      errorMessage: \"\",\n      buttonShown: false\n    };\n  },\n  methods: {\n    removedVoteMachineOption(VoteMachineOption) {\n      let index = this.VoteMachineOptions.indexOf(VoteMachineOption);\n      this.VoteMachineOptions.splice(index, 1);\n      this.errorMessage = \"\";\n    },\n\n    addVoteMachineOption() {\n      this.VoteMachineOptions.push({\n        text: this.newVoteMachineOptionText\n      });\n      this.newVoteMachineOptionText = \"\";\n    },\n\n    clearAllVoteMachineOptions() {\n      this.VoteMachineOptions = [];\n      this.errorMessage = \"\";\n    },\n\n    createVoteMachine() {\n      var VoteMachineObject = {\n        question: this.question,\n        VoteMachineOptions: []\n      };\n\n      this.VoteMachineOptions.forEach(element => {\n        var newPollOptionElement = { name: element.text };\n        if (element.text !== \"\") {\n          VoteMachineObject.VoteMachineOptions.push(newPollOptionElement);\n        }\n      });\n    },\n\n    isCreateVoteMachineDisabled() {\n      return (\n        this.VoteMachineOptions === null ||\n        this.VoteMachineOptions.length < 2 ||\n        this.question === \"\"\n      );\n    }\n  }\n};\n</script>\n\n<style>\n.large-input {\n  box-sizing: border-box;\n  width: 500px;\n  max-width: 80%;\n  border-radius: 7px;\n  border: 1px solid #bdc3c7;\n  padding: 10px 20px;\n  outline: none;\n  text-align: center;\n  line-height: 42px;\n  font-size: 15px;\n  margin: 20px;\n  margin-top: 0;\n  font-size: 20px;\n}\n\n.clear-button {\n  margin-bottom: 25px;\n}\n\n.error-message {\n  font-size: 125%;\n  font-weight: bold;\n}\n</style>\n"]}]}